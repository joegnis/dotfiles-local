{
    "elements": [
        "html5"
    ],
    "extends": [
        "html-validate:recommended"
    ],
    "rules": {
        "no-style-tag": "error",
        "void-style": ["error", {
            "style": "selfclosing"
        }],
        "prefer-native-element": ["warn", {
            "exclude": ["button"]
        }]
    }
}
